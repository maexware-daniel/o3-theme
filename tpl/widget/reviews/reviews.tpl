[{if $oxcmp_user}]

    [{block name="widget_reviews_form"}]
        <form action="[{$oViewConf->getSelfActionLink()}]" method="post" id="details__reviews-form" class="details__reviews-form needs-validation collapse" novalidate>

            <div class="d-none">
                [{if $oView->canRate()}]
                    <input type="hidden" name="artrating" value="0">
                    <input type="hidden" name="recommlistrating" value="0">
                [{/if}]
                [{$oViewConf->getHiddenSid()}]
                [{$oViewConf->getNavFormParams()}]
                [{oxid_include_dynamic file="form/formparams.tpl"}]
                <input type="hidden" name="fnc" value="savereview">
                <input type="hidden" name="cl" value="[{$oViewConf->getTopActiveClassName()}]">
                [{if $oDetailsProduct}]
                    <input type="hidden" name="anid" value="[{$oDetailsProduct->oxarticles__oxid->value}]">
                [{else}]
                    [{assign var="_actvrecommlist" value=$oView->getActiveRecommList()}]
                    <input type="hidden" name="recommid" value="[{$_actvrecommlist->oxrecommlists__oxid->value}]">
                [{/if}]

                [{if $sReviewUserHash}]
                    <input type="hidden" name="reviewuserhash" value="[{$sReviewUserHash}]">
                [{/if}]
            </div>

            [{block name="widget_reviews_form_rating"}]
                [{if $oView->canRate()}]
                    <div class="details__reviews-rating">
                        [{section name=star start=1 loop=6}]
                            <div class="star" data-rate-value="[{$smarty.section.star.index}]" title="[{$smarty.section.star.index}] [{if $smarty.section.star.index==1}][{oxmultilang ident="STAR"}][{else}][{oxmultilang ident="STARS"}][{/if}]"></div>
                        [{/section}]
                    </div>
                [{/if}]
            [{/block}]

            [{block name="widget_reviews_form_fields"}]
                <textarea name="rvw_txt" class="form-control"></textarea>
            [{/block}]

            [{block name="widget_reviews_form_buttons"}]
                <button type="submit" class="submitButton btn btn-primary">[{oxmultilang ident="SAVE_RATING_AND_REVIEW"}]</button>
            [{/block}]

        </form>
    [{/block}]

[{/if}]


[{if $oView->getReviews()}]

    [{* AFTER how many reviews should be hidden with a collapse *}]
    [{assign var="collapseAfter" value=4}]

    [{foreach from=$oView->getReviews() item=review name=ReviewsCounter}]

        [{if $smarty.foreach.ReviewsCounter.iteration == $collapseAfter+1}]
            <div class="collapse" id="hidden-reviews">
        [{/if}]

        <div class="details__reviews-review[{if $smarty.foreach.ReviewsCounter.last && $smarty.foreach.ReviewsCounter.iteration <= $collapseAfter}] is-last[{/if}]" itemprop="review" itemscope itemtype="https://schema.org/Review">

            [{block name="widget_reviews_record"}]

                <div class="details__reviews-writer" itemprop="author" itemscope itemtype="https://schema.org/Person">
